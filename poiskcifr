#include <stdio.h>
#include <stdlib.h>


int str_len(const char *str)
{
    int i = 0;
    int d = 0;
    for(i=0; str[i]!='\0'; i++){
        if(str[i]=='0' || str[i]=='1' || str[i]=='2' || str[i]=='3' || str[i]=='4' || str[i]=='5' || str[i]=='6' || str[i]=='7' || str[i]=='8' || str[i]=='9')
            d++;
  }
    return d;
}


int main()
{
    char s[] = "Hello!";
    printf("%d\n", str_len(s));
    return 0;
}
